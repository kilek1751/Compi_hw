void Fibonacci(int number)    
{    
    int n1=0;
    int n2=1;
    int n3;
    int i;
    
    printf("\n%d %d",n1,n2);//printing 0 and 1
    i = 2;
    while(i<number)//loop starts from 2 because 0 and 1 are already printed    
    {    
        n3=n1+n2;
        printf(" %d",n3);
        n1=n2;
        n2=n3;
        i = i + 1;
    }  
    return 0;  
}
